{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "local": "npm run clean-cache &&cross-env CONFIG_ENV=local node server.js",
    "dev": "npm run clean-cache &&cross-env CONFIG_ENV=development node server.js",
    "qa": "npm run clean-cache &&cross-env CONFIG_ENV=qa node server.js",
    "lint": "eslint --ext .js pages src",
    "format": "eslint --fix --ext .js pages src",
    "create-comp": "hygen generator component",
    "create-page": "hygen generator page",
    "create-reducer": "hygen generator reducer",
    "cli": "hygen",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "build:static_export": "rimraf -rf ./out &&npm run build &&next export",
    "build:serve-static": "npm run build:static_export &&npm run serve-static",
    "serve-static": "http-server -i ./out/index.html",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime-corejs2": "^7.6.3",
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "latest",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^4.2.4",
    "axios": "^0.19.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "immer": "^3.2.0",
    "install": "^0.13.0",
    "jss": "^10.0.0",
    "jss-rtl": "^0.3.0",
    "lodash": "^4.17.15",
    "material-ui-popup-state": "^1.4.1",
    "moment": "^2.24.0",
    "net-provider": "^1.3.8",
    "next": "latest",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^1.2.1",
    "next-images": "^1.2.0",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.3",
    "node-sass": "^4.12.0",
    "notistack": "^0.9.5",
    "npm": "^6.11.1",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^3.24.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "store": "^2.0.12",
    "tinycolor2": "^1.4.1",
    "uninstall": "0.0.0",
    "webpack": "^4.40.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "h": "^1.0.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "now": "^16.3.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix --ext .js pages src"
  }
}
